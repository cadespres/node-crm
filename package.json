{
  "name": "crm",
  "version": "1.0.0",
  "description": "CRM proof of concept app using",
  "main": "app.js",
  "scripts": {
    "make-modek": "npx sequelize-cli model:generate --name user --attributes email:string",
    "make-seed": "npx sequelize-cli seed:generate --name demo-user",
    "seed": "npx sequelize-cli db:seed:all",
    "undo-seed": "npx sequelize-cli db:seed:undo",
    "migrate": "npx sequelize-cli db:migrate",
    "down": "npx sequelize-cli db:migrate:undo",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cadespres/node-crm.git"
  },
