  },
  "homepage": "https://github.com/cadespres/node-crm#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "nodemon": "^2.0.18",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.1"
  },
  "devDependencies": {
    "sequelize-cli": "^6.4.1"
  }
